{
    "name": "maz-system",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "SET NODE_ENV=development && yarn run build:css && react-scripts start",
        "build": "SET NODE_ENV=production yarn run build:css react-scripts build",
        "build:css": "postcss src/assets/css/tailwind.css -o src/assets/css/main.css",
        "eject": "react-scripts eject",
        "generate": "node ./generators/bin -n"
    },
    "engines": {
        "node": ">=12.13.0",
        "npm": "6.12.0",
        "yarn": ">=1.9.4"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GMaker01/maz-system"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 9",
        "not op_mini all"
    ],
    "dependencies": {
        "core-js": "^3.6.5",
        "moment": "^2.27.0",
        "node-sass": "^4.14.1",
        "react": "^16.13.1",
        "react-app-polyfill": "^1.0.6",
        "react-dom": "^16.13.1",
        "react-hook-form": "5.1.1",
        "react-hook-form-input": "1.1.10",
        "react-redux": "^7.2.0",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-router-redux": "^4.0.8",
        "react-scripts": "^3.4.1",
        "redux": "^4.0.5",
        "redux-freeze": "^0.1.5",
        "redux-logger": "^3.0.6",
        "redux-promise-middleware": "^6.1.2",
        "redux-thunk": "^2.2.0",
        "tailwindcss": "^1.6.2"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.6",
        "command-line-args": "^5.0.2",
        "pascal-case": "^3.1.1",
        "postcss-cli": "^7.1.1",
        "replace-in-file": "^5.0.2"
    }
}
